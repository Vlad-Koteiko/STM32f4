{
    "cmake.useCMakePresets": "always",
    "cmake.generator": "MinGW Makefiles",
    "cmake.parallelJobs": 4,
    "cmake.configureSettings": {
      "CMAKE_TRY_COMPILE_TARGET_TYPE" : "STATIC_LIBRARY",
    },
    "cmake.configureEnvironment": {
      "CMAKE_EXPORT_COMPILE_COMMANDS": "on"
    },
    "C_Cpp.default.intelliSenseMode": "gcc-arm",
    "cortex-debug.gdbPath": "arm-none-eabi-gdb",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    
    "files.associations": {
      "*.hpp": "cpp",
      "array": "cpp",
      "atomic": "cpp",
      "bit": "cpp",
      "*.tcc": "cpp",
      "cctype": "cpp",
      "chrono": "cpp",
      "clocale": "cpp",
      "cmath": "cpp",
      "cstdarg": "cpp",
      "cstddef": "cpp",
      "cstdint": "cpp",
      "cstdio": "cpp",
      "cstdlib": "cpp",
      "ctime": "cpp",
      "cwchar": "cpp",
      "cwctype": "cpp",
      "deque": "cpp",
      "unordered_map": "cpp",
      "vector": "cpp",
      "exception": "cpp",
      "algorithm": "cpp",
      "functional": "cpp",
      "iterator": "cpp",
      "memory": "cpp",
      "memory_resource": "cpp",
      "numeric": "cpp",
      "optional": "cpp",
      "random": "cpp",
      "ratio": "cpp",
      "string": "cpp",
      "string_view": "cpp",
      "system_error": "cpp",
      "tuple": "cpp",
      "type_traits": "cpp",
      "utility": "cpp",
      "fstream": "cpp",
      "initializer_list": "cpp",
      "iosfwd": "cpp",
      "istream": "cpp",
      "limits": "cpp",
      "new": "cpp",
      "ostream": "cpp",
      "sstream": "cpp",
      "stdexcept": "cpp",
      "streambuf": "cpp",
      "typeinfo": "cpp",
      "bitset": "cpp",
      "cstring": "cpp",
      "exceptio": "cpp",
      "stdexcep": "cpp",
      "compare": "cpp",
      "concepts": "cpp",
      "condition_variable": "cpp",
      "mutex": "cpp",
      "numbers": "cpp",
      "semaphore": "cpp",
      "stop_token": "cpp",
      "thread": "cpp"
    },
    "cmake.options.statusBarVisibility": "visible"
}